IfModule mod_ssl.c>
<VirtualHost *:443>
    ServerAdmin webmaster@localhost
    ServerName fitapp.ge
    ServerAlias www.fitapp.ge
    DocumentRoot /var/www

    WSGIDaemonProcess fitapp user=www-data group=www-data threads=5
    WSGIScriptAlias / /var/www/fitapp/fitapp.wsgi

    <Directory /var/www/fitapp>
        WSGIProcessGroup fitapp
        WSGIApplicationGroup %{GLOBAL}
        Require all granted
    </Directory>

ErrorLog ${APACHE_LOG_DIR}/error.log
CustomLog ${APACHE_LOG_DIR}/access.log combined
    
Include /etc/letsencrypt/options-ssl-apache.conf
SSLCertificateFile /etc/letsencrypt/live/fitapp.ge/fullchain.pem
SSLCertificateKeyFile /etc/letsencrypt/live/fitapp.ge/privkey.pem
</VirtualHost>
</IfModule>